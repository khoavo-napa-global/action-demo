{
  "extends": "solhint:recommended",
  "plugins": [],
  "rules": {
    "compiler-version": ["error", "0.8.9"],
    "func-visibility": [
      "warn",
      {
        "ignoreConstructors": true
      }
    ],
    "mark-callable-contracts": "false",
    "avoid-suicide": "error",
    "avoid-sha3": "warn"
  }
}
